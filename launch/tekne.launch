<launch>
  <!-- Use real time -->
  <param name="use_sim_time" value="false"/>

  <!-- Start MAVROS -->
  <include file="$(find mavros)/launch/apm.launch" />

  <!-- Robot Localization EKF node -->
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization_node" output="screen">
    <rosparam command="load" file="$(find tramola)/config/ekf_se_odom.yaml" />
  </node>

  <!-- Navsat Transform node -->
  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform" output="screen">
    <rosparam command="load" file="$(find tramola)/config/navsat_transform.yaml" />
  </node>

  <!-- Robot state publisher for robot model -->
  <param name="robot_description" command="$(find xacro)/xacro '$(find tramola)/urdf/your_robot.urdf.xacro'" />
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />
</launch>